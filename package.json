{
  "name": "langchat-app-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nodemon -w server/**/*.ts --ext ts --exec ts-node --project tsconfig.server.json server/server",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build app",
    "build": "npm run build:next && npm run build:server",
    "prod": "cross-env NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/next": "^9.0.0",
    "@types/react-dom": "^16.9.6",
    "config": "^3.3.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "next": "9.3.5",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/config": "^0.0.36",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "nodemon": "^2.0.3",
    "ts-node": "^8.9.0",
    "typescript": "^3.8.3"
  }
}
